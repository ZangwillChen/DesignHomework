<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.czw.entity">
    <class name="com.czw.entity.ReserveInfo" table="tb_revinfo">
        <id name="revID" type="java.lang.Long">
            <column name="rev_id" />
            <generator class="native" />
        </id>
        
        <property name="revDate" type="java.lang.String" not-null="false"
                  length="100">
            <column name="rev_date" />
        </property>

        <property name="reserveStatus" type="java.lang.String" not-null="true"
                  length="200" >
            <column name="rev_status" />
        </property>
    </class>
</hibernate-mapping> 